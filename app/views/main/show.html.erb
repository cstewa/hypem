<div id= "left_screen">
  <% @playlists.each do |playlist| %>
    <ul>
      <%= playlist.name %>
      <% playlist.songs.each do |song| %>
        <li>
          <div class= "song_player">
            <%= song.soundcloud_player %>
          </div>
        </li>
      <% end %>
    </ul>
  <% end %>
</div>
